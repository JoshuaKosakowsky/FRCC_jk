(function() {
    'use strict';

    console.log('TamperMonkey script started');

    function simulateTypingAndEnter() {
        console.log('Simulating typing "SFARGFE" and pressing Enter');

        const text = 'SFARGFE';
        let i = 0;

        function typeCharacter() {
            if (i < text.length) {
                const char = text.charAt(i);
                document.dispatchEvent(new KeyboardEvent('keydown', { key: char, bubbles: true }));
                document.dispatchEvent(new KeyboardEvent('keypress', { key: char, bubbles: true }));
                document.dispatchEvent(new KeyboardEvent('keyup', { key: char, bubbles: true }));
                console.log(`Typed character: ${char}`);
                i++;
                setTimeout(typeCharacter, 100); // Delay between keystrokes
            } else {
                // Simulate pressing the Enter key after typing is complete
                document.dispatchEvent(new KeyboardEvent('keydown', { key: 'Enter', bubbles: true }));
                document.dispatchEvent(new KeyboardEvent('keypress', { key: 'Enter', bubbles: true }));
                document.dispatchEvent(new KeyboardEvent('keyup', { key: 'Enter', bubbles: true }));
                console.log('Simulated Enter key press');
            }
        }

        // Start typing characters with delay
        typeCharacter();
    }

    // Wait for 2 seconds before executing the typing action
    setTimeout(() => {
        simulateTypingAndEnter();
    }, 2000); // 2-second delay

    console.log('Script execution delayed for 2 seconds');
})();
