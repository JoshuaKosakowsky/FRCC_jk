            <style>
              <defaultStyles>
                <defaultStyle refStyle="pb"/>
              </defaultStyles>
            </style>
            <contents>
              <list>
                <style>
                  <defaultStyles>
                    <defaultStyle refStyle="ls"/>
                  </defaultStyles>
                </style>
                <query refQuery="Current Holds"/>
                <listColumns>
                  <listColumn>
                    <listColumnTitle>
                      <textItem>
                        <dataSource><staticValue>Hold Name</staticValue></dataSource>
                      </textItem>
                    </listColumnTitle>
                    <listColumnBody>
                      <textItem>
                        <dataSource><dataItemValue refDataItem="NAME"/></dataSource>
                      </textItem>
                    </listColumnBody>
                  </listColumn>
                  <listColumn>
                    <listColumnTitle>
                      <textItem>
                        <dataSource><staticValue>Balance</staticValue></dataSource>
                      </textItem>
                    </listColumnTitle>
                    <listColumnBody>
                      <textItem>
                        <dataSource><dataItemValue refDataItem="BALANCE"/></dataSource>
                      </textItem>
                    </listColumnBody>
                  </listColumn>
                </listColumns>
              </list>
            </contents>
          </pageBody>
        </page>
      </reportPages>

      <promptPages>
        <page name="Prompt Page1">
          <pageBody>
            <contents>
              <block>
                <contents>
                  <table>
                    <tableRows>
                      <tableRow>
                        <tableCells>
                          <tableCell>
                            <contents>
                              <textItem><dataSource><staticValue>Institution</staticValue></dataSource></textItem>
                            </contents>
                          </tableCell>
                          <tableCell>
                            <contents>
                              <selectValue parameter="p_Institution" refQuery="LOV Institution">
                                <useItem refDataItem="VALUE"/>
                              </selectValue>
                            </contents>
                          </tableCell>
                        </tableCells>
                      </tableRow>
                      <tableRow>
                        <tableCells>
                          <tableCell>
                            <contents>
                              <textItem><dataSource><staticValue>Term</staticValue></dataSource></textItem>
                            </contents>
                          </tableCell>
