  <queries>
    <query name="Current Holds">
      <source><model/></source>
      <selection>
        <dataItem name="NAME"><expression>[Receivable Customer].[Hold].[NAME]</expression></dataItem>
        <dataItem name="BALANCE" aggregate="total"><expression>[Receivable Customer].[Receivable Account Detail].[BALANCE]</expression></dataItem>
        <dataItem name="HOLD"><expression>[Receivable Customer].[Hold].[HOLD]</expression></dataItem>
        <dataItem name="ACADEMIC_PERIOD"><expression>[Receivable Customer].[Receivable Account Detail].[ACADEMIC_PERIOD]</expression></dataItem>
        <dataItem name="HOLD_TO_DATE"><expression>[Receivable Customer].[Hold].[HOLD_TO_DATE]</expression></dataItem>
      </selection>
      <detailFilters>
        <detailFilter><filterExpression>[ACADEMIC_PERIOD] in (?p_Academic_Period?)</filterExpression></detailFilter>
        <detailFilter><filterExpression>[HOLD] in (?p_Hold?)</filterExpression></detailFilter>
        <detailFilter><filterExpression>[HOLD_TO_DATE] > current_date</filterExpression></detailFilter>
        <detailFilter><filterExpression>[List of Values].[Multi Source LOV].[VALUE] = ?p_Institution?</filterExpression></detailFilter>
      </detailFilters>
    </query>

    <query name="LOV Institution">
      <source><model/></source>
      <selection>
        <dataItem name="VALUE"><expression>[List of Values].[Multi Source LOV].[VALUE]</expression></dataItem>
        <dataItem name="VALUE_DESCRIPTION"><expression>[List of Values].[Multi Source LOV].[VALUE_DESCRIPTION]</expression></dataItem>
      </selection>
    </query>

    <query name="LOV Academic Period">
      <source><model/></source>
      <selection>
        <dataItem name="VALUE"><expression>[List of Values].[Academic Period LOV].[VALUE]</expression></dataItem>
        <dataItem name="VALUE_DESCRIPTION"><expression>[List of Values].[Academic Period LOV].[VALUE_DESCRIPTION]</expression></dataItem>
      </selection>
      <detailFilters>
        <detailFilter><filterExpression>[VALUE] between 202010 and 203030</filterExpression></detailFilter>
      </detailFilters>
    </query>

    <query name="LOV Holds">
      <source><model/></source>
      <selection>
        <dataItem name="VALUE"><expression>[List of Values].[Hold LOV].[VALUE]</expression></dataItem>
        <dataItem name="VALUE_DESCRIPTION"><expression>[List of Values].[Hold LOV].[VALUE_DESCRIPTION]</expression></dataItem>
      </selection>
    </query>
  </queries>

  <modelPath>/content/folder[@name='Accounts Receivable']/folder[@name='Data Sources']/folder[@name='ODS Custom Packages']/package[@name='Receivable_Customer_C']/model[@name='model']</modelPath>
  <reportName>Finance Holds</reportName>
