<report xmlns="http://developer.cognos.com/schemas/report/16.2/" expressionLocale="en-us" useStyleVersion="11.4">
  <!--RSU-SPC-0093 The report specification was upgraded from version 14.3 to 16.2 -->
  <drillBehavior/>
  <layouts>
    <layout>
      <reportPages>
        <page name="Page1">
          <style>
            <defaultStyles>
              <defaultStyle refStyle="pg"/>
            </defaultStyles>
          </style>
          <pageBody>
            <style>
              <defaultStyles>
                <defaultStyle refStyle="pb"/>
              </defaultStyles>
            </style>
            <contents>
              <list horizontalPagination="true" name="List1" refQuery="Query1">
                <noDataHandler>
                  <contents>
                    <block>
                      <contents>
                        <textItem>
                          <dataSource>
                            <staticValue>No Data Available</staticValue>
                          </dataSource>
                          <style>
                            <CSS value="padding:10px 18px;"/>
                          </style>
                        </textItem>
                      </contents>
                    </block>
                  </contents>
                </noDataHandler>
                <!-- listColumns section unchanged for brevity -->
              </list>
            </contents>
          </pageBody>
          <XMLAttributes>
            <XMLAttribute output="no" name="RS_legacyDrillDown" value="0"/>
          </XMLAttributes>
        </page>
      </reportPages>
      <promptPages>
        <page name="Prompt page1">
          <pageBody>
            <contents>
              <table>
                <tableRows>
                  <tableRow>
                    <tableCells>
                      <tableCell>
                        <contents>
                          <textItem>
                            <dataSource>
                              <staticValue>VPDI: </staticValue>
                            </dataSource>
                          </textItem>
                        </contents>
                      </tableCell>
                      <tableCell>
                        <contents>
                          <selectValue multiSelect="true" parameter="p_Institution" refQuery="QueryVPDI" required="false" selectValueUI="listBox">
                            <useItem refDataItem="VALUE">
                              <displayItem refDataItem="VALUE1"/>
                            </useItem>
                          </selectValue>
                        </contents>
                      </tableCell>
                    </tableCells>
                  </tableRow>
                </tableRows>
              </table>
            </contents>
          </pageBody>
        </page>
      </promptPages>
    </layout>
  </layouts>
  <queries>
    <query name="Query1">
      <source>
        <model/>
      </source>
      <selection>
        <!-- selection items unchanged for brevity -->
      </selection>
      <detailFilters>
        <detailFilter>
          <filterExpression>[HOLD1]='T'</filterExpression>
        </detailFilter>
        <detailFilter>
          <filterExpression>[Hold VPDI1]=[MULTI_SOURCE1]</filterExpression>
        </detailFilter>
        <detailFilter use="optional">
          <filterExpression>[Hold VPDI1] in (?p_Institution?)</filterExpression>
        </detailFilter>
      </detailFilters>
    </query>
    <query name="QueryVPDI">
      <source>
        <model/>
      </source>
      <selection>
        <dataItem aggregate="none" name="VALUE">
          <expression>[List of Values].[Multi Source LOV].[VALUE]</expression>
        </dataItem>
        <dataItem aggregate="none" name="VALUE1" sort="ascending">
          <expression>[List of Values].[Multi Source LOV].[VALUE]</expression>
        </dataItem>
      </selection>
    </query>
  </queries>
  <modelPath>/content/folder[@name='Accounts Receivable']/folder[@name='Data Sources']/folder[@name='ODS Delivered Packages']/package[@name='Receivable Customer']/model[@name='model']</modelPath>
  <reportName>Hold Codes w Acct Bal-All T (Updated)</reportName>
</report>
